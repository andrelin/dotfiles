[user]
  name = Ã˜yvind Marthinsen
  email = EMAIL
[core]
  editor = vim
  excludesfile = ~/.gitignore_global
[alias]
  br = branch
  ci = commit
  co = checkout
  df = diff
  st = status -sb
  prune = fetch --prune
  llg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  sup = log --since '1 day ago' --oneline --author oma@knowit.no  # I know what you did yesterday - great for follow-ups
  ls = log --pretty=format:'%C(yellow)%h %C(blue)%ad%C(red)%d %C(reset)%s%C(green) [%cn]' --decorate --date=short  # pretty one-line log with tags, branches and authors
  lsv = log --pretty=format:'%C(yellow)%h %C(blue)%ad%C(red)%d %C(reset)%s%C(green) [%cn]' --decorate --date=short --numstat    # a verbose ls, shows changed files too
[help]
  autocorrect = 1
[branch "master"]
  remote = origin
  merge = refs/heads/master
[color]
  ui = auto
  status = auto
  branch = auto
# [color "branch"]
#   current = yellow reverse
#   local = yellow
#   remote = green
# [color "diff"]
#   meta = yellow bold
#   frag = magenta bold
#   old = red bold
#   new = green bold
# [color "status"]
#   added = yellow
#   changed = green
#   untracked = cyan
[push]
  default = simple
[remote "origin"]
  push = HEAD
[url "git@github.com:"]
  insteadOf = "gh:"
  pushInsteadOf = "github:"
  pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
  insteadOf = "github:"
[url "git@gist.github.com:"]
  insteadOf = "gst:"
  pushInsteadOf = "gist:"
  pushInsteadOf = "git://gist.github.com/"